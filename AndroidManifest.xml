<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.coepbc"
          android:versionCode="1"
          android:versionName="1.0">
  <uses-sdk android:minSdkVersion="8"/>
  <application android:label="@string/app_name" android:icon="@drawable/row_to_win" android:theme="@style/boat_club_title_theme">
    <activity android:name=".MainActivity"
              android:label="@string/app_name">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>
        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>
    <activity
        android:name=".ContactActivity"
        android:label="@string/contact_us"
        android:icon="@drawable/row_to_win">
      <meta-data
          android:name="android.support.PARENT_ACTIVITY"
          android:value=".MainActivity" />
    </activity>
    <activity android:name=".HistoryActivity">
    </activity>
    <activity android:name=".RegattaActivity">
    </activity>
  </application>
</manifest>
